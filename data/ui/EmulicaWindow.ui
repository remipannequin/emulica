  <object class="GtkToggleAction" id="add_assy">
    <property name="label">Add Assemble Actuator</property>
    <property name="short_label" translatable="yes">Assemble</property>
    <property name="tooltip" translatable="yes">Assemble the loaded product with another one taken according to programs</property>
    <property name="icon_name">emulica-assemble</property>
    <signal name="toggled" handler="on_add_assy_toggled" swapped="no"/>
  </object>
  <object class="GtkToggleAction" id="add_create">
    <property name="label">Add Create Actuator</property>
    <property name="short_label" translatable="yes">Create</property>
    <property name="tooltip" translatable="yes">Create new emulated products into the specified holer</property>
    <property name="icon_name">emulica-create</property>
    <property name="always_show_image">True</property>
    <signal name="toggled" handler="on_add_create_toggled" swapped="no"/>
  </object>
  <object class="GtkToggleAction" id="add_dispose">
    <property name="label">Add Dispose Actuator</property>
    <property name="short_label" translatable="yes">Dispose</property>
    <property name="tooltip" translatable="yes">Remove one product from the specified holder </property>
    <property name="icon_name">emulica-dispose</property>
    <signal name="toggled" handler="on_add_dispose_toggled" swapped="no"/>
  </object>
  <object class="GtkToggleAction" id="add_failure">
