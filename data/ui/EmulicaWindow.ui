    <property name="icon_name">emulica-failure</property>
    <signal name="toggled" handler="on_add_failure_toggled" swapped="no"/>
  </object>
  <object class="GtkToggleAction" id="add_holder">
    <property name="label">Add product holder</property>
    <property name="short_label" translatable="yes">Holder</property>
    <property name="tooltip" translatable="yes">Product holders contain products when they are transformed or are waiting</property>
    <property name="icon_name">emulica-fifo</property>
    <signal name="toggled" handler="on_add_holder_toggled" swapped="no"/>
  </object>
  <object class="GtkToggleAction" id="add_pullobs">
    <property name="label">Add manual observer</property>
    <property name="short_label" translatable="yes">Manual</property>
    <property name="tooltip" translatable="yes">manual observer send a report about the product in a holder when requested</property>
    <property name="icon_name">emulica-pullobs</property>
    <signal name="toggled" handler="on_add_pullobs_toggled" swapped="no"/>
  </object>
  <object class="GtkToggleAction" id="add_pushobs">
